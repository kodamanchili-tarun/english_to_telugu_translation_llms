      "source": [
        "df_sub = load_data('train.en','train.te',60000)\n",
        "train_df, dev_df, test_df = split_data(df_sub, 0.8, 0, 0.2)\n",
        "model,tokenizer = load_model_and_tokinizer()"
      ]
